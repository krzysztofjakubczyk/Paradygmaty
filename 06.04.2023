int Ile_jest(char tab[], int rozmiar) {
	int iloscNiewypelnionych;
	for (int i = 0; i < rozmiar; i++) {
		if (tab[i] = NULL) {
			iloscNiewypelnionych++;
		}
		if (tab[i] == 1) {
			continue;
		}
	}
	return iloscNiewypelnionych;
}

